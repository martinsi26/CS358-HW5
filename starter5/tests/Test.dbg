772#    lw $t0,($sp) # second parameter
776#    beq $t0,$zero,goEsFalse # go return false if null
780#    lw $t1,-12($t0) # vtable pointer
784#    la $t2,CLASS_String # string vtable pointer
788#    bne $t1,$t2,goEsFalse # go return false if not a string
