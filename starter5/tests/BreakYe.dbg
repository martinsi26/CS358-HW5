3464#    lw $t2,($sp)
3468#    beq $t2,$zero,checkCastReturn
    
    # get vtable address of object, abort if less than
    # lower limit or greater then or equal to higher
    # limit
3472#    lw $t2,-12($t2) # vtable address
3476#    bge $t2,$t1,castException
