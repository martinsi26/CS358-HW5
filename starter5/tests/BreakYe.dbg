###############################################################
# hashCode() - library method (class String)
# - returns a hash code for an object
# - produces int that is the object's hash code
#   meaning that they are the same physical object
# - parameters:
#   - $s2 - this-pointer
#   - ($sp) - old-this-pointer
# - return-value:
#   - $t0 - 1 if the objects were the same; 0 otherwise
###############################################################
556:mth_String_hashCode:
556#    move $t0,$s2 # put this pointer in t0

560#    li $t2,0 # current hash value
564#    lw $t1,-4($t0) # negative of number of characters in string
568#    beq $t1,$zero,done_hashCode_String # if zero length => done
572#    addu $t0,$t1 # address of first character in string +12,
                 # not accounting for padding and header words
576#    and $t0,0xfffffffc # adjust for padding
577##...lui $at,65535
581##...ori $at,$at,0xfffc
585##...and $t0,$t0,$at
588#    subu $t1,$t0,$t1 # address at end of string +12
592:loop_hashCode_String:    
592#    sll $t4,$t2,5 # multiply by 32
596#    subu $t2,$t4,$t2 # subtract original, so that it's now multiply by 31
600#    lbu $t3,-12($t0) # current character
604#    addu $t2,$t3 # add in current character
608#    addu $t0,1 # go to next character's address
612#    bne $t0,$t1,loop_hashCode_String # loop back if more
616:done_hashCode_String:    

    # push int on stack
616#    move $t0, $t2 # put $t2 in return slot
620#    jr $ra # return
