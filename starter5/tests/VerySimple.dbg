    # quick test for obvious false
604#    lw $t0,($sp) # second parameter
608#    beq $t0,$zero,goEsFalse # go return false if null
612#    lw $t1,-12($t0) # vtable pointer
616#    la $t2,CLASS_String # string vtable pointer
620#    bne $t1,$t2,goEsFalse # go return false if not a string

    # save $ra, $s2, second parameter
